<div class="container">
    <tabs onchange="tabChange1">
        <tab-bar  >
            <text style="font-size: 20px;">看过的人</text>
<!--            <image style="height: 20px;width: 20px;" src="/common/icons/help.png"></image>-->
            <text style="font-size: 20px;">更多</text>
        </tab-bar>
        <tab-content>
            <div style="justify-content: center;flex-direction: column;align-items: center;">
<!--                未发现更多内容-->
                <div style="justify-content: center;flex-direction: column;align-items: center;height: 250px;width: 100%;">
